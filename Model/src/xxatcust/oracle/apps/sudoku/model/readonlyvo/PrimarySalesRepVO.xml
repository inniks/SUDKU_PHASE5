<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PrimarySalesRepVO"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="xxatcust.oracle.apps.sudoku.model.readonlyvo.PrimarySalesRepVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="P_ORG_ID"
    Kind="where"
    Type="java.math.BigDecimal"/>
  <SQLQuery>
    <![CDATA[SELECT JRS.RESOURCE_ID,
    JRS. NAME Resource_name,
     jreb.source_email,
      jreb.source_phone,
      pp.phone_number fax
  FROM JTF_RS_SALESREPS JRS, 
  jtf_rs_resource_extns jreb,
  (select parent_id,PHONE_NUMBER from per_phones where phone_type='WF') pp,
  (select parent_id,PHONE_NUMBER from per_phones where phone_type='W1') ppfax
WHERE JRS.SALESREP_NUMBER = ASO_UTILITY_PVT.GET_OU_ATTRIBUTE_VALUE ('SALESREP', :P_ORG_ID)
   AND NVL (JRS.STATUS, 'A') = 'A'
    AND jreb.source_id = pp.parent_id(+)
    and jreb.source_id = ppfax.parent_id(+)
     AND jreb.resource_id=  JRS.RESOURCE_ID
   -- and JRS.person_id=pp.parent_id
   --  and JRS.person_id=ppfax.parent_id
   AND TRUNC (SYSDATE) BETWEEN NVL (TRUNC(JRS.START_DATE_ACTIVE),TRUNC(SYSDATE)) AND  NVL(TRUNC(JRS.END_DATE_ACTIVE),TRUNC (SYSDATE))
   AND ROWNUM = 1]]>
  </SQLQuery>
  <ViewAttribute
    Name="ResourceId"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="BIGINT"
    AliasName="RESOURCE_ID"
    Expression="RESOURCE_ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ResourceName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="240"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="RESOURCE_NAME"
    Expression="RESOURCE_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="240"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SourceEmail"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2000"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SOURCE_EMAIL"
    Expression="SOURCE_EMAIL"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2000"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SourcePhone"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2000"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SOURCE_PHONE"
    Expression="SOURCE_PHONE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2000"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Fax"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="60"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="FAX"
    Expression="FAX"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="60"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="ResourceId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="xxatcust.oracle.apps.sudoku.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
